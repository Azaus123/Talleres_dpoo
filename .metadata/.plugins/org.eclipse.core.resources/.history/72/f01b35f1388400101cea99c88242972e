package actividad_construir_uml;

import java.awt.Color;

public interface IParte {
	int darX();
	int dary();
	int darAncho();
	String darTipo();
	Color darColor(); ///de clase Color, la importamos
	void cambiarX(int pX); //recibe un parametro de tipo int
	void cambiarY(int pY);
	boolean estaDentro(int pX, int pY); //recibe dos parametros
	void pintar(Graphics2D pGraphics); //de clase Graphics2D
	void pintarSombreada(Graphics2D pGraphics);
	void pintarSeleccionada(Graphics2D pGraphics);
	void guardar(PrintWriter pEscritor); //de clase PrintWriter
	

}
